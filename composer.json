{
    "name": "aflores/formcomponent",
    "description": "Form Component for Laravel Framework",
    "license": "proprietary",
    "authors": [
        {
            "name": "Alan Flores",
            "email": "alanfm89@gmail.com",
            "homepage": "https://github.com/aflores04"
        }
    ],
    "homepage": "https://github.com/aflores04/formcomponent",
    "keywords": ["Laravel", "FormComponent"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "FormComponent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FormComponent\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FormComponent\\FormComponentServiceProvider"
            ],
            "aliases": {
                "FormComponent": "FormComponent\\Facades\\FormComponent"
            }
        }
    }
}
